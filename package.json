    "@storybook/react": "^9.0.8",
    "@storybook/react-vite": "^9.0.8",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
